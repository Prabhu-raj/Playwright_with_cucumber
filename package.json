{
  "name": "playwright_with_cucumber",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "all_tests": "cucumber-js src/features/**/*.feature --require-module ts-node/register --require src/step-definitions/**/*.ts --require src/**/**/**/**/utils/cucumber-timeout.ts",
    "contactus_tests": "cucumber-js src/features/Contact_Us.feature --require-module ts-node/register --require src/step-definitions/**/*.ts --require src/**/**/**/**/utils/cucumber-timeout.ts",
    "Login_tests_with_hooks": "cucumber-js src/features/Login.feature --require-module ts-node/register --require ./src/step-definitions/hooks/hooks.ts --require ./src/step-definitions/Loginpage_steps.ts --require ./src/utils/cucumber-timeout.ts",
    "Login_tests": "cucumber-js src/features/Login.feature --require-module ts-node/register --require src/step-definitions/**/*.ts --require src/**/**/**/**/utils/cucumber-timeout.ts", 
    "Contactus_tests_smoke": "cucumber-js --require-module ts-node/register --require src/step-definitions/**/*.ts --require src/utils/cucumber-timeout.ts src/features/Contact_Us.feature --tags '@smoke'",
    "smoke": "cucumber-js --require-module ts-node/register --require src/step-definitions/**/*.ts --require src/utils/cucumber-timeout.ts src/features/**/*.feature --tags '@smoke'",
    "regression": "cucumber-js --require-module ts-node/register --require src/step-definitions/**/*.ts --require src/utils/cucumber-timeout.ts src/features/**/*.feature --tags '@regression'", 
    "ignore": "cucumber-js --require-module ts-node/register --require src/step-definitions/**/*.ts --require src/utils/cucumber-timeout.ts src/features/**/Contact_Us.feature --tags 'not @ignore'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@cucumber/cucumber": "^12.1.0",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.2.0",
    "ts-node": "^10.9.2"
  }
}
